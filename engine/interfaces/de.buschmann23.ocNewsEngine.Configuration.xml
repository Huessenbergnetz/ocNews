        <arg name="stats" type="a{sv}" direction="out" />
    </method>
    <signal name="gotStatistics">
        <annotation name="com.trolltech.QtDBus.QtTypeName.In0" value="QVariantMap"/>
        <arg name="stats" type="a{sv}" direction="out" />
    </signal>
    <method name="cleanCertificates"/>
